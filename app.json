{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app-icon/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/app-splash/splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#295341"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app-icon/adaptive-icon.png",
        "backgroundColor": "#295341"
      }
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#295341"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/app-icon/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "googleFont": {
        "apiKey": "YOUR_GOOGLE_FONT_API_KEY"
      }
    }
  }
}
